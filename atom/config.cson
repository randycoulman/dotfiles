"*":
  autosave:
    enabled: true
  core:
    audioBeep: false
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
    projectHome: "/Users/randy/src"
  editor:
    fontFamily: "Hack"
    fontSize: 19
    invisibles: {}
    showIndentGuide: true
    tabType: "soft"
  emmet: {}
  "exception-reporting":
    userId: "30e6f2d0-42d3-ab77-4984-dde4c431f019"
  "expand-region": {}
  "git-blame": {}
  "git-diff":
    showIconsInEditorGutter: true
    rspecAllCommand: "bundle exec rspec --tty spec"
    rspecFileCommand: "bundle exec rspec --tty {relative_path}"
    rspecSingleCommand: "bundle exec rspec --tty {relative_path}:{line_number}"
  "git-plus": {}
  linter:
    errorPanelHeight: 26
  "linter-eslint": {}
  "linter-sass-lint":
    noConfigDisable: true
  "markdown-deluxe":
    fontFamily: "Hack"
  "package-sync":
    forceOverwrite: true
  "ruby-test":
    specFramework: "rspec"
    testFramework: "minitest"
  tabs:
    enableVcsColoring: true
  "terminal-plus":
    core: {}
    style: {}
  "toggle-quotes": {}
  "vim-mode":
    startInInsertMode: true
  welcome:
    showOnStartup: false
"source.gfm":
  editor:
    softWrap: true
